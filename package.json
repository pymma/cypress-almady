  "keywords": [
    "cypress",
    "automation",
    "almady",
    "IHM"
  ],
  "author": "Fofana Cheick",
  "license": "ISC",
  "dependencies": {
    "cypress": "^4.7.0",
    "cypress-file-upload": "^4.0.7",
    "mocha": "^7.2.0",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.0.3"
  }
}
