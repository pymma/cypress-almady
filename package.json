{
  "name": "cypress-almady-test",
  "version": "1.0.0",
  "description": "test almady IHM with cypress tools",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run:test" : "cypress run --spec 'cypress/integration/authentification/Log.spec.js'",
    "cy:run:default": "cypress run",
